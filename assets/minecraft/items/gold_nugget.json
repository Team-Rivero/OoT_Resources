{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/gold_nugget"
    },
    "cases": [
      {
        "when": "deku_nut",
        "model": {
          "type": "model",
          "model": "item/deku_nut"
        }
      },
      {
        "when": "bomb",
        "model": {
          "type": "model",
          "model": "item/bomb"
        }
      }
    ]
  }
}